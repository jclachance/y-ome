#!/bin/bash

cd "$(dirname "$0")"

dropdb yome

createdb yome \
    && ./create_tables \
    && ../sources/ecocyc/load \
    && ../sources/ecogene/load \
    && ../sources/uniprot/load \
    && ../sources/refseq/load \
    && ../sources/regulondb/load \
    && ../sources/ecolime/load \
    && ../sources/manual/load \
    && ./load_yome
